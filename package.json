{
  "name": "metal-router",
  "version": "0.0.5",
  "description": "Routing solution to link URLs to Metal.js components using HTML5 History API.",
  "license": "BSD",
  "repository": "metal/metal-router",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/Router.js",
  "main": "lib/Router.js",
  "files": [
    "lib",
    "src/**/*.js",
    "test/**/*.js"
  ],
  "scripts": {
    "build:demo": "metal build -s demos/basic/src/**/*.js -d demos/basic/build --soySrc demos/**/*.soy --soyDest demos --soyDeps src/RouterSoy.soy",
    "compile": "babel --presets metal -d lib/ src/",
    "prepublish": "gulp soy && npm run compile",
    "test": "gulp test"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal-promise": "^1.0.0-rc.2",
    "metal-soy": "^1.0.0-rc.13",
    "senna": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-metal": "^4.0.0",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.7.1",
    "metal-cli": "^1.0.1",
    "metal-karma-config": "^2.1.0"
  }
}
